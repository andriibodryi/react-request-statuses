{
  "name": "react-request-statuses",
  "version": "1.0.4",
  "description": "Utility library for handle request statuses",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.ts",
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "NODE_ENV=production run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint ."
  },
  "keywords": [
    "react-hooks",
    "react",
    "react-native",
    "request",
    "redux",
    "react-redux",
    "status",
    "statuses"
  ],
  "author": "Andrii Bodryi",
  "license": "MIT",
  "ava": {
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ]
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-redux": "^7.0.0"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^1.6.1",
    "@types/jest": "^26.0.24",
    "@types/lodash.get": "^4.4.6",
    "@types/node": "^12.20.18",
    "@types/react": "^16.14.11",
    "@types/react-redux": "^7.1.18",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "gh-pages": "^2.2.0",
    "install": "^0.13.0",
    "jest": "^26.6.3",
    "lodash.get": "^4.4.2",
    "microbundle-crl": "^0.13.11",
    "npm": "^7.20.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "react": "^16.13.1",
    "react-dom": "^17.0.2",
    "react-lifecycle-hook": "^1.0.0",
    "react-redux": "^7.2.4",
    "typescript": "^3.9.10"
  },
  "files": [
    "dist"
  ]
}
